{
  "collection": {
    "name": "Book Cover Repository",
    "description": "This collection stores metadata and file references for book cover images.",
    "version": "1.0",
    "last_updated": "2026-02-12",
    "endpoints": {
      "/items": "List all items",
      "/users": "List all users",
      "/items/:id/users": "Nested route: users related to an item (users where item_id = :cover_id)"
    },
    "schemas": {
      "items": {
        "id": "number",
        "book_title": "string",
        "isbn": "string",
        "image_url": "string",
        "image_format": "string",
        "resolution": "string",
        "upload_date": "date"
      },
      "users": {
        "user_id": "number",
        "first_name": "string",
        "last_name": "string",
        "email": "string",
        "role": "string",
        "created_at": "date",
		"item_id": "number (foreign key to items.id)"
      }
    },
